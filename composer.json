{
  "name": "trinity/grid",
  "description": "Trinity Grid bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Trinity Team",
      "email": "info@modpreneur.com"
    }
  ],

  "require": {
    "php": ">=7",
    "symfony/framework-bundle": "~2.8 | ~3.0",
    "nette/utils": "~2.3",
    "trinity/framework": "dev-master",
    "twig/extensions" : "~1.3",
    "friendsofsymfony/user-bundle": "dev-master",
    "jms/serializer-bundle": "~1.1",
    "trinity/search": "dev-master",
    "sensio/framework-extra-bundle": "~3.0",
    "trinity/settings": "dev-master"
  },

  "require-dev": {
    "phpunit/phpunit": "~5.1",
    "mockery/mockery": "~0.9",
    "symfony/var-dumper": "~3.0",
    "fzaninotto/faker": "1.5.*"
  },

  "autoload": {
    "psr-4": { "Trinity\\Bundle\\GridBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },

  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
