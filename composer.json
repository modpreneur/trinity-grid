{
  "name": "trinity/grid",
  "description": "Trinity Grid bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin Matejka",
      "email": "matejka@webvalley.cz"
    }
  ],

  "require": {
    "php": ">=7.1",
    "twig/extensions" : "~1.3",
    "jms/serializer-bundle": "~2.0",
    "sensio/framework-extra-bundle": "~3.0",
    "friendsofsymfony/rest-bundle": "~2.0",
    "doctrine/doctrine-fixtures-bundle": "~2.3",
    "knplabs/doctrine-behaviors": "~1.3",
    "symfony/security-bundle": "~3.0",
    "symfony/twig-bundle": "~3.0",
    "symfony/form": "~3.0",
    "symfony/browser-kit": "~3",
    "symfony/yaml": "~3.0",
    "doctrine/cache": "~1",
    "trinity/settings": "~1.2",
    "trinity/logger" : "~1.0",
    "trinity/search": "dev-master",
    "trinity/utils": "dev-master",
    "trinity/core-interfaces": "~1.0"
  },

  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~0.9",
    "symfony/var-dumper": "~3.0",
    "fzaninotto/faker": "~1.5",
    "phpstan/phpstan": "~0.7"
  },

  "autoload": {
    "psr-4": { "Trinity\\Bundle\\GridBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },

  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
